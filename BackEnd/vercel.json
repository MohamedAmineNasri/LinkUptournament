{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.)",
        "dest": "/app.js"
      },
      {
        "src": "/(.)",
        "dest": "/app.js"
      }
    ]
  }